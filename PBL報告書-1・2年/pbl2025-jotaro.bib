@inproceedings{buffaCanYouDAW2024,
  title = {Can You {{DAW}} It {{Online}}?},
  booktitle = {{{IS2}} 2024 - {{IEEE International Symposium}} on the {{Internet}} of {{Sounds}} 2024 / 1st {{IEEE International Workshop}} on the {{Musical Metaverse}} ({{IEEE IWMM}})},
  author = {Buffa, Michel and Demont, Samuel},
  year = 2024,
  month = sep,
  address = {Erlangen, Germany},
  urldate = {2025-11-20},
  abstract = {この記事では、Web Audio、Web MIDI、Web Assembly、Web ComponentsなどのW3C標準、およびWeb Audio Modules（WAM）プラグイン標準（別名「Web用VST」）を活用したオープンソースのデジタルオーディオワークステーション（DAW）であるWAM-Studioを紹介します。2022年から開発が進められているWAM-StudioとそのフォークであるAttuneは、既に複数の出版物で取り上げられています。本稿では、パフォーマンス（レイテンシー管理を含む）に焦点を当て、これまでサポートされていなかったMIDIトラックなどの新機能の追加について詳しく説明します。現在、ほとんどのオンラインDAWはクローズドソースであり、少数のオープンソースプロジェクトはメンテナンスされていないか、本稿で紹介する作業ほど完成度が高くありません。WAM-Studioは、Web標準のオープンソースデモンストレーターとして開発されており、正しく実現するのが複雑な機能（他のトラックとの同期によるオーディオおよびMIDI録音、プロジェクトのオフラインレンダリングなど）の設計と実装例が含まれています。オンラインデモとソースコードのGitHubリポジトリが利用可能です。},
  keywords = {Online Digital Audio Workstation,Web Audio,Web Audio Modules 12 Demonstration,Web Audio Plugins,Web MIDI,WebAssembly},
  note = {https://inria.hal.science/hal-04695237}
}

